	}

                
TH      {       
        vertical-align: baseline;
        font-size:      .9em;
        font-weight:    bold;
        word-spacing:   normal;
        letter-spacing: normal;
        text-transform: none;
        font-family:   Arial, Helvetica, sans-serif;
        font-size-adjust:       .48;
	color: #2e965d;
        }       
                
        /* end header type */   

        /* begin affordances */ /* 4 - trop a */



EM      {       
        font-style:     italic;
        font-weight:    bold;
	color: #2e965d;
        }       
                
STRONG  {       
        font-weight:    bold;
        font-style:     normal;
 	color: #2e965d;
         }       
                
EM STRONG, STRONG EM    {       
        font-family:   Arial, Helvetica, sans-serif;
        font-size-adjust:       .48;
        font-size:      1.58em;
        font-weight:    500;
        text-transform: uppercase;
        font-style:     normal;
        font-weight:    bolder;
        color:  #2e965d;
        }       

I      {       
        font-style:     italic;
        font-weight:    bold;
 	color: #2e965d;
        }       
                
B       {       
        font-weight:    bold;
        font-style:     normal;
 	color: #2e965d;
        }       
                
I B, B I    {       
        font-family:   Arial, Helvetica, sans-serif;
        font-size:      1.58em;
        font-weight:    500;
        font-style:     normal;
        font-weight:    bolder;
        color:  #2e965d;
        }       

                

A:link  {       
        text-decoration:        none;
        font-weight:    bold;
        color:  #099;
        }       
                
A:visited       {       
        text-decoration:        none;
        font-weight:    bold;
        color:  #3ebc77;
        }       
                
A:active        {       
        text-decoration:        none;
        font-weight:    bold;
        color:  #F00;
        }       
                
A:hover {       
        text-decoration:        none;
        color:  #F00;
        background:     #F00;
        }       
                
A:offsite       {       
        text-decoration:        none;
        font-weight:    normal;
        color:  #C00;
        background:     #FFC;
        }       
