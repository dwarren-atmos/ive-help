       /* begin body type */   /* 1 - humanist sans a */

BODY    {       
        font-size:      .9em;
        font-weight:    normal;
        word-spacing:   normal;
        letter-spacing: normal;
        text-transform: none;
        font-family:    Arial, Helvetica, sans-serif;
        font-size-adjust:       .48;
	margin-left:    2%;
	margin-right:   2%;
	background:	#ffffff;
	color: #2e965d;
	}

P, CAPTION, BLOCKQUOTE, UL, OL, DL, DD, LI, DIR, MENU, HR    {       
        /* inherit from body */ 
	color:  #2e965d ;

      }       

OL LI   {       
        list-style-type:        decimal;
	color: #2e965d;
        }       

OL OL LI        {       
        list-style-type:        lower-alpha;
 	color: #2e965d;
       }       

OL OL OL LI     {       
        list-style-type:        lower-roman;
 	color: #2e965d; 
       }       

TABLE, TBODY, TR, TD    {       
        font-size:      .9em;
        word-spacing:   normal;
        letter-spacing: normal;
        text-transform: none;
        font-family:   Arial, Helvetica, sans-serif;
        font-size-adjust:       .58;
	color: #2e965d; 
       }       

        /* end body type */     



        /* begin header type */ /* 4 - modern sans a */
                
H1      {       
        font-family:   Arial, Helvetica, sans-serif;
        font-size-adjust:       .48;
        font-size:      2em;
        font-weight:    700;
        font-style:     normal;
        text-decoration:        none;
        word-spacing:   normal;
        letter-spacing: normal;
        text-transform: none;
	color:		#2e965d;
        }       
                
